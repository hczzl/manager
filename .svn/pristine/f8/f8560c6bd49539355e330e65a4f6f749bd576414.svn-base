public enum TaskFinishFlagEnum
{
    COMMIT("提交","-1"),
    UNDONE("未完成，执行中","0"),
    COMPLETE("任务完成","1"),
    STOP("任务中止","2"),
    COMPLETE_APPROVE("任务完成审批中","5"),
    STOP_APPROVE("任务中止审批中","6"),
    CHANGE_APPROVE("任务变更审批中","7");
    private String name;
    private String code;

    TaskFinishFlagEnum(String name, String code) {
        this.name = name;
        this.code = code;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getCode() {
        return code;
    }

    public void setCode(String code) {
        this.code = code;
    }
}
